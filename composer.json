{
    "name": "php-etl/bucket",
    "description": "This library implements the Extract-Transform-Load pattern asynchronously in PHP with the help of iterators and generators",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Kiboko SAS",
            "homepage": "http://kiboko.fr"
        },
        {
            "name": "Gr√©gory Planchat",
            "email": "gregory@kiboko.fr"
        }
    ],
    "minimum-stability": "dev",
    "prefer-stable": true,
    "require": {
        "php": "^8.2",
        "php-etl/bucket-contracts": "^0.2@dev"
    },
    "autoload": {
        "psr-4": {
            "Kiboko\\Component\\Bucket\\": "src/"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-main": "0.3.x-dev",
            "dev-feature/qualityflow-improvments": "0.3.x-dev"
        }
    },
    "require-dev": {
        "phpspec/phpspec": "^7.0",
        "friends-of-phpspec/phpspec-code-coverage": "6.x-dev",
        "phpunit/phpunit": "^9.5 || ^10.0",
        "phpstan/phpstan": "^1.10",
        "friendsofphp/php-cs-fixer": "^3.0",
        "infection/infection": "^0.26.18",
        "rector/rector": "^0.15",
        "johnkary/phpunit-speedtrap": "*",
        "mybuilder/phpunit-accelerator": "*",
        "phpunit/php-invoker": "*"
    },
    "config": {
        "bin-dir": "bin",
        "allow-plugins": {
            "infection/extension-installer": true
        }
    }
}
